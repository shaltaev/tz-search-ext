{
  "name": "tz-search-ext",
  "version": "0.0.4",
  "description": "tz-search-ext",
  "content_scripts": [
    {
      "matches": ["*://*.google.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
      "default_icon": "icon-16.png",
      "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": ["storage", "activeTab", "*://*.google.com/*"],
  "manifest_version": 2
}
